{
  "name": "my-project",
  "source": "",
  "scripts": {
    "start": "parcel static/index.html --no-cache --open",
    "build": "parcel build static/index.html && node ./static/server.js",
    "lint:ts": "eslint --ext .ts ./",
    "lint:ts:fix": "eslint --ext .ts ./ --fix",
    "lint:scss": "npx stylelint \"**/*.sass\"",
    "lint:scss:fix": "npx stylelint \"**/*.sass\" --fix"
  },
  "devDependencies": {
    "@parcel/transformer-inline-string": "2.7.0",
    "@parcel/transformer-sass": "2.7.0",
    "@typescript-eslint/eslint-plugin": "5.42.1",
    "@typescript-eslint/parser": "5.42.1",
    "eslint": "^8.28.0",
    "eslint-config-airbnb": "19.0.4",
    "express": "4.18.2",
    "parcel": "2.7.0",
    "parcel-plugin-clean-easy": "1.0.2",
    "parcel-plugin-static-files-copy": "2.6.0",
    "parcel-transformer-hbs": "1.0.4",
    "stylelint": "^14.15.0",
    "typescript": "4.8.4"
  },
  "dependencies": {
    "eslint-config-airbnb-base": "^15.0.0",
    "handlebars": "4.7.7",
    "stylelint-scss": "^4.3.0",
    "uuid": "9.0.0"
  },
  "@parcel/transformer-css": {
    "cssModules": {
      "dashedIdents": true
    }
  },
  "parcelCleanPaths": [
    "dist",
    ".cache"
  ],
  "parcelCleanUnsafePaths": true,
  "staticFiles": {
    "staticPath": "static",
    "watcherGlob": "**"
  }
}
